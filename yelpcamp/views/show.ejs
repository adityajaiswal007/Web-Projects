<% include partials/header %>

<div class="container">
	<h1><%= showCamp.name %></h1>
	<img src="<%= showCamp.image %>">
	<p><%= showCamp.description %></p>
	<% showCamp.comment.forEach(function(comments){ %>
	<p>
		<strong><%= comments.author %></strong>-<%= comments.text %>
	</p>
	<% }) %>
</div>

<% include partials/footer  %>